# array_search

{{code:php
    $array = array(0 => 'a', 1 => 'b', 2 => 'c', 3 => 'd');

    $result = array_search('a', $array);
    print $result."\n";
    // => 0

    $result = array_search('c', $array);
    print $result."\n";
    // => 2
}}

